pipeline {
  agent any 
  
    stages {
      stage ('this is first stage '){
        steps {
          echo "this is the forst command"
          }
         }
       stage ('this is second  stage '){
        steps {
          echo "this is the forst command"
          }
          }
         }
    parameters { string(name: 'DEPLOY_ENV', defaultValue: 'staging', description: '')  
                  boolean(name: 'this is bolena parameterr', defaultValye: 'boolean'. description: '')
            }
      
    
        }
